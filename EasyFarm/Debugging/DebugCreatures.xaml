<Window x:Class="EasyFarm.Debugging.DebugCreatures"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:data="clr-namespace:EasyFarm"
        Title="DebugCreatures" Height="347" Width="476">
    <Grid>
        <DockPanel>
            <ListBox Name="lstMobNames" DockPanel.Dock="Left" SelectionChanged="lstMobNames_SelectionChanged" ItemsSource="{Binding Units}" />
            <ListBox Name="lstMobData" DockPanel.Dock="Right" />
        </DockPanel>
    </Grid>
</Window>
